{
  {% if isPlugin %}
  "miniprogramRoot": "dist/miniprogram",
  "pluginRoot": "dist/plugin",
  "compileType": "plugin",
  {% else %}
  "miniprogramRoot": "dist/",
  "compileType": "miniprogram",
  {% endif %}
  "setting": {
    "newFeature": true,
    "urlCheck": true,
    "es6": false,
    "postcss": false,
    "minified": false,
    "autoAudits": false
  },
  "appid": "<$ appid $>",
  "projectname": "<$ name $>",
  "description": "<$ description $>",
  "condition": {}
}
